<aura:component implements="force:appHostable, flexipage:availableForAllPageTypes, flexipage:availableForRecordHome, force:hasRecordId, forceCommunity:availableForAllPageTypes" access="global" controller="getUserInfo">

<aura:attribute name="userId" type="String"/>
<aura:attribute name="userName" type="String" default="s"/>
<aura:handler name="init" value="{!this}" action="{!c.init}"/>

<div class="slds-align_absolute-center">
<img src="{!$Resource.Header}"/>
</div>
<div class="slds-text-align_right">
<p><c:logoutbutton userName="{!v.userName}" /></p>
</div>

   
</aura:component>

